{
  "manifest_version": 2,

  "name": "Circuit Dark Mode",
  "description": "Dark Mode for  Circuit. Extension not official",
  "version": "1.0.1",
  "content_scripts": [
    {
      "matches": ["https://eu.yourcircuit.com/*"], 
      "js": ["js/script.js"],
      "css" : ["css/style.css"]
    }
  ],
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
